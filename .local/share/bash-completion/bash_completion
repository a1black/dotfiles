# user source compat completion directory definitions
compat_dir=${BASH_COMPLETION_USER_COMPAT_DIR:-"$(dirname ${BASH_SOURCE[0]})/completions"}
if [[ -d $compat_dir && -r $compat_dir && -x $compat_dir ]]; then
    for i in "$compat_dir"/*; do
        [[ ${i##*/} != @($_backup_glob|Makefile*|$_blacklist_glob) \
            && -f $i && -r $i ]] && . "$i"
    done
fi
unset compat_dir i _blacklist_glob
