{
    "name": "Color scheme for powerline with gitstatus extension",
    "groups": {
        "gitstatus":                 { "fg": "solarized:base3",         "bg": "solarized:blue",   "attrs": [] },
        "gitstatus_branch":          { "fg": "solarized:base3",         "bg": "solarized:blue",   "attrs": [] },
        "gitstatus_branch_clean":    { "fg": "solarized:base3",         "bg": "solarized:green",  "attrs": [] },
        "gitstatus_branch_dirty":    { "fg": "solarized:base03",        "bg": "solarized:yellow", "attrs": [] },
        "gitstatus_branch_detached": { "fg": "mediumpurple",            "bg": "gray2",            "attrs": [] },
        "gitstatus_tag":             { "fg": "solarized:cyan",          "bg": "gray2",            "attrs": [] },
        "gitstatus_behind":          { "fg": "gray10",                  "bg": "gray2",            "attrs": [] },
        "gitstatus_ahead":           { "fg": "gray10",                  "bg": "gray2",            "attrs": [] },
        "gitstatus_staged":          { "fg": "solarized:green",         "bg": "gray2",            "attrs": [] },
        "gitstatus_unmerged":        { "fg": "solarized:red",           "bg": "gray2",            "attrs": [] },
        "gitstatus_changed":         { "fg": "solarized:orange",        "bg": "gray2",            "attrs": [] },
        "gitstatus_untracked":       { "fg": "solarized:orange",        "bg": "gray2",            "attrs": [] },
        "gitstatus_stashed":         { "fg": "solarized:blue",          "bg": "gray2",            "attrs": [] },
        "gitstatus:divider":         { "fg": "gray8",                   "bg": "gray2",            "attrs": [] },

        "background":                { "fg": "solarized:base1",         "bg": "solarized:base03", "attrs": [] },
        "background:divider":        { "fg": "solarized:base1",         "bg": "solarized:base03", "attrs": [] },

        "hostname":                  { "fg": "solarized:yellow",        "bg": "solarized:orange", "attrs": [] },
        "environment":               { "fg": "solarized:base3",         "bg": "solarized:green",  "attrs": [] },
        "mode":                      { "fg": "solarized:base3",         "bg": "solarized:green",  "attrs": ["bold"] },
        "attached_clients":          { "fg": "solarized:base3",         "bg": "solarized:green",  "attrs": [] },

        "battery_full":              { "fg": "solarized:base3",         "bg": "solarized:green",  "attrs": ["bold"] },
        "battery_empty":             { "fg": "solarized:base3",         "bg": "solarized:red",    "attrs": ["bold"] },
        "battery_gradient":          { "fg": "solarized:base3",         "bg": "mediumpurple",     "attrs": ["bold"] },

        "date":                      { "fg": "solarized:base3",         "bg": "solarized:blue",   "attrs": [] },
        "time":                      { "fg": "solarized:base3",         "bg": "solarized:blue",   "attrs": [] },
        "time:divider":              { "fg": "solarized:base3",         "bg": "solarized:blue",   "attrs": [] },

        "system_load":               { "fg": "gray8",                   "bg": "gray0",            "attrs": [] },
        "system_load_gradient":      { "fg": "green_yellow_orange_red", "bg": "solarized:base03", "attrs": [] },

        "continuation":              { "fg": "solarized:base1",         "bg": "solarized:blue",   "attrs": [] },
        "continuation:current":      { "fg": "solarized:base1",         "bg": "solarized:base03", "attrs": [] }
    }
}
