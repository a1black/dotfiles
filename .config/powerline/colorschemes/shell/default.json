{
    "name": "Color scheme for powerline with gitstatus extension",
    "groups": {
        "gitstatus":                 { "fg": "white",           "bg": "darkblue",         "attrs": [] },
        "gitstatus_branch":          { "fg": "white",           "bg": "darkblue",         "attrs": [] },
        "gitstatus_branch_clean":    { "fg": "white",           "bg": "green",            "attrs": [] },
        "gitstatus_branch_dirty":    { "fg": "black",           "bg": "yellow",           "attrs": [] },
        "gitstatus_branch_detached": { "fg": "mediumpurple",    "bg": "gray2",            "attrs": [] },
        "gitstatus_tag":             { "fg": "darkcyan",        "bg": "gray2",            "attrs": [] },
        "gitstatus_behind":          { "fg": "gray10",          "bg": "gray2",            "attrs": [] },
        "gitstatus_ahead":           { "fg": "gray10",          "bg": "gray2",            "attrs": [] },
        "gitstatus_staged":          { "fg": "green",           "bg": "gray2",            "attrs": [] },
        "gitstatus_unmerged":        { "fg": "brightred",       "bg": "gray2",            "attrs": [] },
        "gitstatus_changed":         { "fg": "mediumorange",    "bg": "gray2",            "attrs": [] },
        "gitstatus_untracked":       { "fg": "brightestorange", "bg": "gray2",            "attrs": [] },
        "gitstatus_stashed":         { "fg": "darkblue",        "bg": "gray2",            "attrs": [] },
        "gitstatus:divider":         { "fg": "gray8",           "bg": "gray2",            "attrs": [] },

        "background":                { "fg": "solarized:base1", "bg": "solarized:base03", "attrs": [] },
        "background:divider":        { "fg": "solarized:base1", "bg": "solarized:base03", "attrs": [] },

        "hostname":                  { "fg": "brightyellow",    "bg": "mediumorange",     "attrs": [] },
        "environment":               { "fg": "white",           "bg": "darkestgreen",     "attrs": [] },
        "mode":                      { "fg": "darkestgreen",    "bg": "brightgreen",      "attrs": ["bold"] },
        "attached_clients":          { "fg": "white",           "bg": "darkestgreen",     "attrs": [] },

        "battery_full":              { "fg": "green",           "bg": "green",            "attrs": ["bold"] },
        "battery_empty":             { "fg": "red",             "bg": "red",              "attrs": ["bold"] },
        "battery_gradient":          { "fg": "white",           "bg": "mediumpurple",     "attrs": ["bold"] },

        "date":                      { "fg": "white",           "bg": "darkblue",         "attrs": [] },
        "time":                      { "fg": "white",           "bg": "darkblue",         "attrs": [] },
        "time:divider":              { "fg": "white",           "bg": "darkblue",         "attrs": [] },

        "system_load":               { "fg": "gray8",           "bg": "gray0",            "attrs": [] },
        "system_load_gradient":      { "fg": "green_yellow_orange_red", "bg": "solarized:base03", "attrs": [] },

        "continuation":              { "fg": "solarized:base1", "bg": "solarized:blue",   "attrs": [] },
        "continuation:current":      { "fg": "solarized:base1", "bg": "solarized:base03", "attrs": [] }
    }
}
